/* start pattern of navigate based concept jowoscript technology */
jowo-navigate {
	width: 100%;
    display: inline-block;
    position: absolute;
    background: repeating-linear-gradient(360deg, #00288b, #00288b 100px);
	line-height: 1.3;
    margin: 5px 0px;
    border: 2px solid #00288b;
    border-radius: 25px;
    top: 0;
    left: 0;
    text-align: center;
    font-weight: bold;
    color: #fff;
    font-size: x-large;
    text-transform: auto;
}
button-navigate {
	padding: 1px 6px;
}
/* end pattern of navigate based concept jowoscript technology */

/* start pattern of font based concept jowoscript technology */
jowo-bold, .bold {
	font-weight:bold;
} 
jowo-bolder, .bolder {
	font-weight:bolder;
}
jowo-italic, .italic {
	font-style: italic;
}
jowo-topline, .topline {
	text-decoration: overline;
	text-decoration-style: solid;
}
jowo-midline, .midline {
	text-decoration: line-through;
	text-decoration-style: solid;
}
jowo-underline, .underline {
	text-decoration: underline;
	text-decoration-style: solid;
}
jowo-bothline, .bothline {
	text-decoration: underline overline;
	text-decoration-style: solid;
}
jowo-toplinewavy, .toplinewavy {
	text-decoration: overline;
	text-decoration-style: wavy;
}
jowo-midlinewavy, .midlinewavy {
	text-decoration: line-through;
	text-decoration-style: wavy;
}
jowo-underlinewavy, .underlinewavy {
	text-decoration: underline;
	text-decoration-style: wavy;
}
jowo-bothlinewavy, .bothlinewavy {
	text-decoration: underline overline;
	text-decoration-style: wavy;
}
jowo-toplinedash, .toplinedash {
	text-decoration: overline;
	text-decoration-style: dashed;
}
jowo-midlinedash, .midlinedash {
	text-decoration: line-through;
	text-decoration-style: dashed;
}
jowo-underlinedash, .underlinedash {
	text-decoration: underline;
	text-decoration-style: dashed;
}
jowo-bothlinedash, .bothlinedash {
	text-decoration: underline overline;
	text-decoration-style: dashed;
}
jowo-toplinedot, .toplinedot {
	text-decoration: overline;
	text-decoration-style: dotted;
}
jowo-midlinedot, .midlinedot {
	text-decoration: line-through;
	text-decoration-style: dotted;
}
jowo-underlinedot, .underlinedot {
	text-decoration: underline;
	text-decoration-style: dotted;
}
jowo-bothlinedot, .bothlinedot {
	text-decoration: underline overline;
	text-decoration-style: dotted;
}
jowo-toplinedouble, .toplinedouble {
	text-decoration: overline;
	text-decoration-style: double;
}
jowo-midlinedouble, .midlinedouble {
	text-decoration: line-through;
	text-decoration-style: double;
}
jowo-underlinedouble, .underlinedouble {
	text-decoration: underline;
	text-decoration-style: double;
}
jowo-bothlinedouble, .bothlinedouble {
	text-decoration: underline overline;
	text-decoration-style: double;
}
/* end pattern of font based concept jowoscript technology */

/* start pattern of position based concept jowoscript technology */
jowo-left, .left {
	float: left;
	display: grid;
}
jowo-right, .right {
	float: right;
	display: grid;
}
jowo-font-auto, .font-auto {
	text-align: auto;
	display: grid;
}
jowo-font-center, .font-center {
	text-align: center;
	display: grid;
}
jowo-font-left, .font-left {
	text-align: left;
	display: grid;
}
jowo-font-right, .font-right {
	text-align: right;
	display: grid;
}
/* end pattern of position based concept jowoscript technology */

/* start pattern of width and hight based concept jowoscript technology */
jowo-auto-width, .auto-width {
	width: initial;
	min-width: max-content;	
}
jowo-max-width, .max-width {
	width: 100%;
	min-width: max-content;	
}
jowo-min-width, .min-width {
	width: 0%;
    max-width: min-content;
}

jowo-auto-height, .auto-height {
	height: initial;
	min-height: max-content;
}
jowo-max-height, .max-height {
	height: 100%;
    min-height: max-content;
}
jowo-min-height, .min-height {
	height: 0%;
    max-height: min-content;
}
/* end pattern of width and hight based concept jowoscript technology */

/* start pattern elements based concept jowoscript technology */
jowo-html {
	display: block;
}
jowo-head {
	display: none;
}
jowo-body {
	display: block;
	margin: 8px;
}
jowo-div {
	display: block;
}
jowo-label {
	cursor: default;
}
jowo-p {
	display: block;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
}
jowo-table-caption {
	display: table-caption;
    text-align: -webkit-center;
}
jowo-table, table, .table {
	border-collapse: separate;
    text-indent: initial;
    border-spacing: 2px;
	font-size: inherit;
}
jowo-colgroup {
    display: table-column-group;
}
jowo-col {
    display: table-column;
}
jowo-thead {
    display: table-header-group;
    vertical-align: middle;
    border-color: inherit;
}
jowo-tbody {
	display: table-row-group;
    vertical-align: middle;
    border-color: inherit;
}
jowo-tr {
	display: table-row;
    vertical-align: inherit;
    border-color: inherit;
}
jowo-th {
	display: table-cell;
    vertical-align: inherit;
    font-weight: bold;
    text-align: -internal-center;
}
jowo-td {
	display: table-cell;
    vertical-align: inherit;
}
jowo-tfoot {
	display: table-footer-group;
    vertical-align: middle;
    border-color: inherit;
}
jowo-select {
	-webkit-writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0px;
    text-shadow: none;
    text-align: start;
    white-space: pre;
    -webkit-rtl-ordering: logical;
    cursor: default;
    font: 400 13.3333px Arial;
}
jowo-select:not(:-internal-list-box) {
    overflow: visible !important;
}
jowo-option {
	font-weight: normal;
    display: block;
    white-space: nowrap;
    min-height: 1.2em;
    padding: 0px 2px 1px;
}
jowo-figure {
    display: block;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 40px;
    margin-inline-end: 40px;
}
jowo-figcaption {
    display: block;
}
jowo-ul {
    display: block;
    list-style-type: disc;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    padding-inline-start: 40px;
}
jowo-ul jowo-li {
	list-style-type: disc;
}
jowo-li {
	display: list-item;
    text-align: -webkit-match-parent;
}
jowo-li::marker {
    unicode-bidi: isolate;
    font-variant-numeric: tabular-nums;
    text-transform: none;
    text-indent: 0px !important;
    text-align: start !important;
    text-align-last: start !important;
}
jowo-textarea {
	-webkit-writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0px;
    text-shadow: none;
    display: inline-block;
    text-align: start;
    appearance: auto;
    background-color: -internal-light-dark(rgb(255, 255, 255), rgb(59, 59, 59));
    -webkit-rtl-ordering: logical;
    flex-direction: column;
    resize: auto;
    cursor: text;
    white-space: pre-wrap;
    overflow-wrap: break-word;
    column-count: initial !important;
    margin: 0em;
    font: 400 13.3333px Arial;
    border-width: 1px;
    border-style: solid;
    border-color: -internal-light-dark(rgb(118, 118, 118), rgb(133, 133, 133));
    border-image: initial;
    padding: 2px;
}
jowo-input {
	-webkit-writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0px;
    text-shadow: none;
    display: inline-block;
    text-align: start;
    appearance: auto;
    background-color: -internal-light-dark(rgb(255, 255, 255), rgb(59, 59, 59));
    -webkit-rtl-ordering: logical;
    cursor: text;
    margin: 0em;
    font: 400 13.3333px Arial;
    padding: 1px 2px;
    border-width: 2px;
    border-style: inset;
    border-color: -internal-light-dark(rgb(118, 118, 118), rgb(133, 133, 133));
    border-image: initial;
}
jowo-password, jowo-input[type="password" i] {
    -webkit-text-security: disc !important;
    padding: 1px 2px;
    padding-top: 1px;
    padding-right: 2px;
    padding-bottom: 1px;
    padding-left: 2px;
}
jowo-radio, jowo-input[type="radio" i] {
    background-color: initial;
    cursor: default;
    appearance: auto;
    box-sizing: border-box;
    margin: 3px 3px 0px 5px;
    padding: initial;
    border: initial;
}
jowo-checkbox, jowo-input[type="checkbox" i] {
    background-color: initial;
    cursor: default;
    appearance: auto;
    box-sizing: border-box;
    margin: 3px 3px 3px 4px;
    padding: initial;
    border: initial;
}
jowo-input[type="button" i] {
	appearance: auto;
    user-select: none;
    white-space: pre;
    align-items: flex-start;
    text-align: center;
    cursor: default;
    color: -internal-light-dark(black, white);
    background-color: -internal-light-dark(rgb(239, 239, 239), rgb(59, 59, 59));
    box-sizing: border-box;
    padding: 1px 6px;
    border-width: 2px;
    border-style: outset;
    border-color: -internal-light-dark(rgb(118, 118, 118), rgb(133, 133, 133));
    border-image: initial;
}
jowo-button {
	appearance: auto;
    -webkit-writing-mode: horizontal-tb !important;
    text-rendering: auto;
    color: -internal-light-dark(black, white);
    letter-spacing: normal;
    word-spacing: normal;
    text-transform: none;
    text-indent: 0px;
    text-shadow: none;
    display: inline-block;
    text-align: center;
    align-items: flex-start;
    cursor: default;
    background-color: -internal-light-dark(rgb(239, 239, 239), rgb(59, 59, 59));
    box-sizing: border-box;
    margin: 0em;
    font: 400 13.3333px Arial;
    padding: 1px 6px;
    border-width: 2px;
    border-style: outset;
    border-color: -internal-light-dark(rgb(118, 118, 118), rgb(133, 133, 133));
    border-image: initial;
}
/* end pattern elements based concept jowoscript technology */
